package Queue;
import java.util.*;
public class queue1 {
//    First in First out
//    Last in Last Out
public static void main(String[] args) {
    Queue<Integer> q =   new LinkedList<>();
    q.add(1);
    q.add(2);
    q.add(3);
    q.add(4);
    q.add(5);
    System.out.println(q);
//    To remove the element in place of pop we use remove or poll
    q.remove();
    System.out.println(q);
    q.poll();
    System.out.println(q);

//    To access the top element we use either peek or element
    System.out.println(q.peek());
    System.out.println(q.element());

//    To check the size of queue
    System.out.println(q.size());
    q.remove();
    System.out.println(q.size());

    System.out.println(q.isEmpty());

}

}
